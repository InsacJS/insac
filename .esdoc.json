{
  "source": "./lib",
  "destination": "./docs/",
  "includes": ["\\.js$"],
  "excludes": ["apidoc/template"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "undocumentIdentifier": { "enable": false },
        "unexportedIdentifier": { "enable": true },
        "lint": { "enable": true },
        "coverage": { "enable": true },
        "typeInference": { "enable": true },
        "brand": {
          "title": "Insac Framework",
          "description": "Framework de creaci√≥n de servicios web.",
          "repository": "https://github.com/insacjs/insac",
          "site": "http://insacjs.com"
        },
        "manual": {
          "index": "./manual/index.md",
          "files": [
            "./manual/overview.md",
            "./manual/installation.md",
            "./manual/tutorial.md"
          ]
        }
      }
    }
  ]
}
